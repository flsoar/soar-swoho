{
    "name": "soar/swoho",
    "description": "Ultra-lightweight serverless framework to quickly build micro-service monomers.",
    "keywords": ["framework", "serverless", "micro-service"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "flsoar",
            "email": "lvfenglinsoar@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.75",
        "illuminate/pagination": "^8.75",
        "nikic/fast-route": "^1.3"
    },

    "autoload": {
        "classmap": [

        ],
        "psr-4": {
            "Soar\\Swoho\\Base\\":"./src/base/",
            "Soar\\Swoho\\Common\\":"./src/common/",
            "Soar\\Swoho\\Core\\":"./src/core/",
            "Soar\\Swoho\\Facade\\":"./src/facade/"
        },
        "files": [
            "./src/common/functions.php"
        ]
    },
    "minimum-stability":"dev",
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master",
        "symfony/var-dumper": "^6.0"
    }
}
